let node make1(n)() = 
  forward(n) i in 0 to n-1 returns ([|o|]) do o = i done

let node sum(x) returns (o)
  forward (xi in x) returns (o) do
    o = xi + (0 fby o)
  done

let f () =
  forward(10) (i in 0 to 9) returns (o)
    do o = 0 -> pre o + i done

let node main1() returns ()
  assert (f() = sum(run (make1(10)) ()))
